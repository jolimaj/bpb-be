{
  "name": "backend-boiler-plate",
  "version": "1.0.0",
  "description": "About\r Description\r This a boiler plate for nodejs, express and postgres sql backend development.",
  "main": "index.js",
  "Author": "Jolima Javin-Software Engineer",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node server.js",
    "start": "node server.js npm install npx sequelize-cli db:migrate npx sequelize-cli db:seed:all",
    "db:generate": "npx sequelize-cli model:generate",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:seed:generate": "npx sequelize-cli seed:generate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:migrate:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jolimaj/backend-boiler-plate.git"
